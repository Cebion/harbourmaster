{
    "#info": {
        "version": 1,
        "name": "Default Theme",
        "creator": "Tekkenfede and Kloptops"
    },

    "#base": {
        "font": "DejaVuSans.ttf",
        "font-scale[hires]": 2.0,
        "font-scale[5:3,hires]": 3.0
    },

    "#pallet": {
        "background": [200, 200, 200, 255],
        "background_fade": [200, 200, 200, 32],
        "list_text": [0, 0, 0, 255],
        "list_selected": [255, 0, 0, 255],
        "button": [200, 200, 200, 255]
    },

    "#resources": {
        "nine.png": {
            "button":  [  0,   0, 320, 167],
            "dialog":  [  0, 169, 320, 161],
            "box":     [  0, 332, 320, 106],
            "boxfill": [  0, 439, 320, 106],
            "arrow_r": [ 11, 559,  42,  42],
            "arrow_l": [ 11, 559,  42,  42, 1],
            "circle":  [ 64, 547,  62,  62],
            "bar":     [373, 225, 222,  18]
        },
        "buttons.png": {
            "_A":        [  0,   0, 180, 180],
            "_B":        [180,   0, 180, 180],
            "_X":        [360,   0, 180, 180],
            "_Y":        [540,   0, 180, 180],
            "start":     [  0, 180, 360, 180],
            "select":    [360, 180, 360, 180],
            "_L":        [  0, 360, 270, 180],
            "_R":        [270, 360, 270, 180],
            "unchecked": [720,   0, 180, 180],
            "checked":   [900,   0, 180, 180],
            "more":      [720, 180, 180, 180],
            "left":      [900, 180,  90, 180],
            "right":     [990, 180,  90, 180],
            "up":        [900, 360, 180,  90],
            "down":      [900, 450, 180,  90]
        }
    },

    "#elements": {
        "background": {
            "area": [ 0.0, 0.0, 1.0, 1.0 ],
            "fill": "background",
            "border": 8,
            "font-size": 36,
            "font-color": [ 0, 0, 0 ]
        },
        "small_button": {
            "fill": "button",
            "outline": [ 0, 0, 0, 255 ],
            "thickness": 1,
            "font-size": 16,
            "font-color": [ 0, 0, 0 ]
        },
        "button_bar_filters": {
            "area": [ 0.5, 0.93, 0.95, 1.0 ],
            "font-size": 16,
            "font-color": [ 0, 0, 0 ],
            "bar": [
                "_A",
                "Accept",
                "_B",
                "Back",
                "_X",
                "Filter"
            ]
        },
        "button_bar": {
            "area": [ 0.60, 0.93, 0.95, 1.0 ],
            "font-size": 16,
            "font-color": [ 0, 0, 0 ],
            "bar": [
                "_A",
                "Accept",
                "_B",
                "Back"
            ]
        },

        "button_bar2": {
            "area": [ 0.40, 0.93, 0.95, 1.0 ],
            "font-size": 16,
            "font-color": [ 0, 0, 0 ],
            "bar": [
                "_A",
                "Install",
                "_B",
                "Back"
            ]
        },
        "button_bar3": {
            "area": [ 0.5, 0.93, 0.95, 1.0 ],
            "font-size": 16,
            "font-color": [ 0, 0, 0 ],
            "bar": [
                "_B",
                "Back"
            ]
        },
        "option_list": {
            "area": [ 0.1, 0.25, 0.9, 0.9 ],
            "border": 8,
            "font-size": 30,
            "font-color": "list_text",
            "select-color": "list_selected",
            "text-clip": false,
            "thickness": 10
        }
    },

    "ports_list": {
        "#element:background": {},

        "port_info": {
            "area": [ 0.0, 0.0, 0.40, 1.0 ],
            "fill": [ 230, 230, 230 ],
            "thickness": 10,
            "border": 8,
            "font-size": 24,
            "font-color": [ 0, 0, 0 ],
            "text-wrap": true
        },

        "ports_list": {
            "area": [0.0, 0.1, 0.4, 0.95],
            "border-x": 16,
            "roundness": 10,
            "font-size": 14,
            "font-color": "list_text",
            "select-color": "list_selected",
            "text-clip": true,
            "text-wrap": false,
            "autoscroll": "slide",
            "scroll-speed": 30,
            "scroll-delay-start": 500,
            "scroll-delay-end":   500
        },
        "ports_list[hires]": {
            "area": [0.0, 0.1, 0.3, 0.95]
        },

        "port_desc": {
            "text": "{port_info.description}\n\nGenres: {port_info.genres}\nDownload Size: {port_info.download_size}\nRuntime: {port_info.runtime} ({port_info.runtime_status})",
            "area": [ 0.5, 0.52, 0.95, 0.9 ],
            "font-size": 11,
            "font-color": [ 0, 0, 0 ],

            "text-wrap": true,
            "autoscroll": "marquee",
            "scroll-speed": 60,
            "scroll-delay-start": 1500,
            "scroll-delay-end":   1500
        },

        "port_title": {
            "text": "{port_info.title}",
            "area": [0.5, 0.02, 0.90, 0.10],
            "font-size": 32,
            "font-color": [ 255, 0, 0 ],
            "autoscroll": null,
            "text-wrap": false,
            "text-clip": false,
            "align": "topleft"
        },

        "port_image": {
            "area": [0.4, 0.1, 1.0, 0.5],
            "image": "no-image.jpg"
        },

        "#element:button_bar_filters": {}
    },

    "port_info": {
        "#element:background": {},
       
        "free_space": {
            "area": [ 0.0, 0.1, 0.2, 0.2 ],
            "fill": [ 230, 230, 230 ],
            "text": "Free Space: {system.free_space}",
            "thickness": 6,
            "border": 8,
            "font-size": 11,
            "font-color": [ 0, 0, 0 ],
            "text-wrap": true
        },

        "port_info": {
            "area": [ 0.0, 0.68, 1.0, 1.0 ],
            "fill": [ 230, 230, 230 ],
            "text": "{port_info.description}\n\nPorter: {port_info.porter}\nGenres: {port_info.genres}\nDownload Size: {port_info.download_size}\nRuntime: {port_info.runtime} ({port_info.runtime_status})",
            "thickness": 6,
            "border": 8,
            "font-size": 11,
            "font-color": [ 0, 0, 0 ],
            "text-wrap": true
        },


        "port_title": {
            "text": "{port_info.title}",
            "area": [ 0.00, 0.00, 1.00, 0.1 ],
            "font-size": 24,
            "font-color": [ 255, 0, 0 ],
            "autoscroll": null,
            "text-wrap": false,
            "align": "center"
        },

        "port_image": {
            "area": [ 0.51, 0.1, 0.5, 0.65 ],
            "image": "no-image.jpg",
            "align": "center"
        },

        "#element:small_button:ok_button": {
            "area": [ 0.0, 0.0, 0.0, 0.0 ],
            "text": "Okay"
        },

        "#element:small_button:back_button": {
            "area": [ 0.0, 0.0, 0.0, 0.0 ],
            "text": "Back"
        },

        "#element:button_bar2": {}
    },

    "main_menu": {
        "#element:background": {},

        "#element:option_list": {
            "align": "center",
            "list": [
                "Install All Ports",
                "Install Ready to Run Ports",
                "Uninstall Ports",
                "Option Menu",
                "Exit"
            ],
            "option": [
                "install",
                "install-rtr",
                "uninstall",
                "option",
                "exit"
            ],
            "info": [
                "Install a new port onto your device.",
                "Remove a port that has been installed onto your device",
                "PortMaster options",
                "Exit Portmaster and return to your game list."
            ]
        },
        "#element:button_bar": {},

        "system_time": {
            "text": "Time: {system.time_24hr}\nCFW: {system.cfw_name} ({system.cfw_version})\nDevice: {system.device_name}\nBattery: {system.battery_level}",
            "font-size": 13,
            "font-color": [ 0, 0, 0 ],
            "area": [ 0.02, 0.05, 0.95, 0.95 ],
            "align": "topleft"
        },
        "system_time_low": {
            "text": "Space: {system.free_space} free / {system.total_space} total\nPM: {system.portmaster_version}\nHM: {system.harbourmaster_version}",
            "font-size": 11,
            "font-color": [ 0, 0, 0 ],
            "area": [ 0.02, 0.05, 0.95, 0.95 ],
            "align": "bottomleft"
        }

    },

    "option_menu": {
        "#element:background": {},

        "#element:option_list": {
            "list": [
                "Update ports",
                "- * -",
                "- * -",
                "Back"
            ],
            "option": [
                "update-ports",
                "option-2",
                "option-3",
                "back"
            ]
        },

        "#element:button_bar": {}
    },

    "filter_list": {
        "filter_list": {
            "area": [0.25, 0.25, 0.75, 0.75],
            "fill": [230, 230, 230],
            "border": 5,
            "font-size": 13,
            "font-color": "list_text",
            "select-color": "list_selected",
            "list": []
        }
    },

    "message_window": {
        "#element:background": {},

        "message_text": {
            "area": [0.05, 0.05, 0.95, 0.8],
            "fill": [230, 230, 230],
            "thickness": 6,
            "text-wrap": true,
            "border": 10,
            "font-size": 24,
            "font-color": [0, 0, 0],
            "align": "bottomleft",
            "text": ""
        },

        "progress_bar": {
            "area": [0.05, 0.82, 0.95, 0.90],
            "fill": [230, 230, 230],
            "progress-fill": [120, 120, 240],
            "text": "{system.progress_text}",
            "align": "midleft",
            "font-size": 14,
            "font-color": [0, 0, 0],
            "border": 3,
            "thickness": 2
        },

        "progress_bar_amount": {
            "area": [0.05, 0.82, 0.95, 0.90],
            "text": "{system.progress_amount}",
            "align": "midright",
            "font-size": 14,
            "font-color": [0, 0, 0],
            "thickness": 2,
            "border": 3
        },

        "#element:button_bar": {}
    },

    "message_box": {
        "#element:background": {},

        "message_text": {
            "area": [0.05, 0.5, 0.95, 0.8],
            "fill": [230, 230, 230],
            "thickness": 6,
            "border": 8,
            "font-size": 24,
            "font-color": [0, 0, 0],
            "align": "center",
            "text": "",
            "text-wrap": true
        },

        "#element:small_button:ok_button": {
            "area": [0.0, 0.0, 0.0, 0.0],
            "text": "Okay"
        },

        "#element:small_button:back_button": {
            "area": [0.0, 0.0, 0.0, 0.0],
            "text": "Back"
        },

        "#element:button_bar3": {}
    }
}